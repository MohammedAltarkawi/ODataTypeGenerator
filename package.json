{
  "name": "odata-types-generator",
  "version": "1.11.21",
  "description": "A tool to convert OData metadata to TypeScript types",
  "main": "src/index.ts",
  "bin": {
    "odatatypegenerator": "./bin/odatatypegenerator.js",
    "generate-types": "./scripts/config-run.js"
  },
  "scripts": {
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "generate-types": "node ./scripts/config-run.js",
    "odatatypegenerator": "node ./bin/odatatypegenerator.js"
  },
  "keywords": [
    "SAPUI5",
    "TypeScript",
    "OData",
    "Types"
  ],
  "author": "Mohammed Altarkawi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MohammedAltarkawi/ODataTypeGenerator.git"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "js-yaml": "^4.1.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@types/xml2js": "^0.4.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  }
}